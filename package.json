{
  "name": "@tolgee/figma-plugin",
  "version": "0.0.1",
  "description": "Tolgee Figma Plugin",
  "main": "code.js",
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "schema": "openapi-typescript http://localhost:8080/v3/api-docs/Accessible%20with%20API%20key --output ./src/setup/client/apiSchema.generated.ts",
    "icons": "svg-icon build --source icons.json --target ./src/setup --name icons",
    "eslint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "tsc": "tsc --noEmit",
    "prettier": "prettier . --write"
  },
  "author": "Tolgee",
  "license": "MIT",
  "devDependencies": {
    "@create-figma-plugin/build": "^2.1.5",
    "@create-figma-plugin/tsconfig": "^2.1.5",
    "@figma/plugin-typings": "^1.55.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.32.0",
    "jest": "^29.3.1",
    "openapi-typescript": "^6.1.0",
    "prettier": "^2.8.2",
    "svg-icon": "^0.8.2",
    "typescript": "*"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "2.1.5",
    "@create-figma-plugin/utilities": "2.1.5",
    "clsx": "^1.2.1",
    "preact": "^10.11.3",
    "react-query": "^3.39.2",
    "use-context-selector": "^1.4.1"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "main": "src/setup/main.ts",
    "ui": "src/setup/ui.tsx",
    "id": "1",
    "name": "Tolgee"
  }
}
